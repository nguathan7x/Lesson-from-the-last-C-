#include<iostream>
using namespace std;
int main()
{
long long n;
cin>>n;
if(n%2==0)//1 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
if(n%3==0&&n%5==0)//2 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
if(n%3==0&&n%7!=0)//3 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
if(n%3==0||n%7==0)//4 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
if(n>30&&n<50)//5 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
if((n>=30)&&(n%2==0||n%3==0||n%5==0))//6 	
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
long long x=n%10;
if(n>=10&&n<100)//7 
{if(x==2||x==3||x==5||x==7)
cout<<"YES"<<"\n";
}else
cout<<"NO"<<"\n";
if(n<=100&&n%23==0)//8 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
if(n<10||n>20)//9 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
if(x%3==0)//10 
cout<<"YES"<<"\n";
else
cout<<"NO"<<"\n";
}
