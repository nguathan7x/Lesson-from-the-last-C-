#include <bits/stdc++.h>
 using namespace std;
 int main() {
     int N;
     cin >> N;
     int souoc=0;
     int s;
     for (int i=1;i<=N;i++){
         if(N%i==0){
             souoc+=1;
         }
     }
     cout << souoc << "\n";
     for (int i=1;i<=N;i++){
         if(N%i==0){
             cout << i <<" ";
         }
     }
    return 0;
 }
