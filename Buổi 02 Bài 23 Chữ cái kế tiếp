#include<iostream>
#include<iomanip>
#include<math.h>
using namespace std;
int main()
{
	char x;
	cin >> x;
	if (x >= 65 && x < 90)
	{
		cout << (char)(x+33) << '\n';
	}
	else if (x == 90)
	{
		cout << (char)(x +7) << '\n';
	}
	else if (x >= 97 && x <122)
	{
		cout <<(char)(x+1) << '\n';
	}
	else if (x == 122)
	{
		cout << (char)(x - 25) << '\n';
	}
	else 
	{
		cout << "INVALID" << '\n';
	}
	return 0;
}
